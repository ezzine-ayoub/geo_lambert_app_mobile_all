<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="seq_expense_spending" model="ir.sequence">
        <field name="name">Expense Spending</field>
        <field name="code">expense.spending</field>
        <field name="prefix">Note.F</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_expense_replenishment" model="ir.sequence">
        <field name="name">Expense Replenishment</field>
        <field name="code">expense.replenishment</field>
        <field name="prefix">Alim.C</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="close_preperty_server" model="ir.actions.server">
        <field name="name">Genarate Caisse</field>
        <field name="model_id" ref="project.model_project_project" />
        <field name="binding_model_id" ref="project.model_project_project" />
        <field name="state">code</field>
        <field name="code">action = env["hr.expense.account"].create_account_project(record)</field>
        <field name="groups_id" eval="[(4, ref('hr_expense_caisse.group_expense_caisse_administrator'))]"/>
    </record>

</odoo>